try:
    marks = int(input("Enter student marks: "))
    
    if marks > 90:
        print("S grade")
    elif marks > 80:
        print("A grade")
    elif marks > 70:
        print("B grade")
    elif marks > 60:
        print("C grade")
    elif marks > 50:
        print("D grade")
    else:
        print("Fail")
except ValueError as er:
    print("Invalid input. Please enter a valid integer.", er)
except Exception as er:
    print("An error occurred:", er)
